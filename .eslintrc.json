{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "react-app",
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/react-in-jsx-scope": "off" // Corrected typo here
  }
}
